<FlyoutPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
            xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:views="clr-namespace:SimpleMAUICRUD.pages.tabs"
            xmlns:locals="clr-namespace:SimpleMAUICRUD.pages"
            x:Class="SimpleMAUICRUD.MainFlyoutPage">
    <FlyoutPage.Flyout>
        <locals:FlyoutMenuPage x:Name="flyoutPage" />
    </FlyoutPage.Flyout>
    <FlyoutPage.Detail>
        <NavigationPage>
            <x:Arguments>
                <views:TabPage1 />
            </x:Arguments>
        </NavigationPage>
    </FlyoutPage.Detail>
</FlyoutPage>
